include "server_topics.bits"

namespace wise::kernel

message syn_peer_up 
{
	topic server.cluster.syn_peer_up;

	u16 domain = 0;
}

message syn_peer_down
{
	topic server.cluster.syn_peer_down;

	u16 domain = 0;
}

message syn_actor_up
{
	topic server.cluster.syn_actor_up;

	u64 id = 0;
	string type;
	string name;
}

message syn_actor_down
{
	topic server.cluster.syn_actor_down;

	u64 id = 0;
}
